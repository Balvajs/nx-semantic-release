{
  "version": 2,
  "projects": {
    "app-a": {
      "root": "apps/app-a",
      "sourceRoot": "apps/app-a/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-a",
            "main": "apps/app-a/src/main.ts",
            "tsConfig": "apps/app-a/tsconfig.app.json",
            "assets": ["apps/app-a/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/app-a/src/environments/environment.ts",
                  "with": "apps/app-a/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "app-a:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/app-a/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-a"],
          "options": {
            "jestConfig": "apps/app-a/jest.config.js",
            "passWithNoTests": true
          }
        },
        "semantic-release": {
          "executor": "@theunderscorer/nx-semantic-release:semantic-release",
          "options": {
            "dryRun": false,
            "buildTarget": "app-a:build",
            "repositoryUrl": "../../git-server/repos/project.git",
            "github": false,
            "ci": false,
            "outputPath": "dist/apps/app-a",
            "commitMessage": "chore(app-a): release ${nextRelease.version} [skip ci]\\n\\n${nextRelease.notes}",
            "branches": ["*"]
          }
        }
      },
      "tags": []
    },
